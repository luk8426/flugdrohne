  <snippet name="der_mah">
   <global>prevX = 0
prevY = 0
is_first = true</global>
   <function>if (is_first) then
  is_first = false
  prevX = time
  prevY = value
end

dx = time - prevX
dy = value - prevY
prevX = time
prevY = value

return dy/dx</function>
   <linked_source>battery_status/discharged_mah</linked_source>
  </snippet>
 </customMathEquations>
